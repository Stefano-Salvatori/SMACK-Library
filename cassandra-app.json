{
  "id": "cassandra",
  "container": {
    "docker": {
      "image": "cassandra:latest",
      "network": "HOST",
      "ports": [
        7199,
        7000,
        7001,
        9160,
        9042
      ],
      "privileged": true,
      "forcePullImage": true
    }
  },
  "env": {
    "CASSANDRA_CLUSTERNAME": "cassandra-cluster",
    "CASSANDRA_SEEDS": "0.0.0.0"
  },
  "cpus": 2,
  "mem": 2048,
  "instances": 1
}
